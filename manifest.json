{
  "name": "Chrome Plugin Javascript Execution",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://sixth.dk/*"
  ],
  "action": {
    "default_title": "Chrome Plugin Javascript Execution"
  }
}